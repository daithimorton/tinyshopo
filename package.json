{
  "name": "tinyshopo",
  "description": "Tiny Shopo",
  "version": "1.0.0",
  "author": "Daithi @Beautifwhale",
  "dependencies": {
    "gatsby": "2.0.15",
    "gatsby-plugin-favicon": "3.1.4",
    "gatsby-plugin-google-analytics": "2.0.8",
    "gatsby-plugin-react-helmet": "3.0.3",
    "gatsby-plugin-stripe-checkout": "1.0.5",
    "gatsby-source-stripe": "2.0.5",
    "moment": "2.22.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-ga": "^2.5.7",
    "react-helmet": "5.2.0",
    "react-image-gallery": "^0.8.12",
    "stripe": "6.17.0",
    "uuid": "3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:damorton/tinyshopo.git"
  },
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build && npm run lambda:build",
    "develop": "gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently --kill-others \"npm run develop\" \"npm run lambda:serve\"",
    "lambda:build": "netlify-lambda build src/lambda",
    "lambda:serve": "netlify-lambda serve src/lambda",
    "clean": "rm -rf .cache node_modules"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "netlify-lambda": "^1.0.3",
    "prettier": "1.14.2"
  }
}
